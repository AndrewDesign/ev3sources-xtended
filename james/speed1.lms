vmthread  MAIN
{
  DATA8   Run
  MOVE8_8(1,Run)

  DATA8   layer
  DATA8   nos
  MOVE8_8(0,layer)
  MOVE8_8(15,nos)


  // Initialize vireo to run the file with the given path.
  VIREO_INIT('../prjs/output/speed1.via')

  // Execute VIREO_STEP while the return value Run is nonzero.
Loop:
  VIREO_STEP(Run)
  JR_TRUE(Run,Loop)

  OUTPUT_READY(layer,nos)
}

